<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfiloCentralinista">

    <TextView
        android:id="@+id/nome_text_profilo_volontario"
        android:layout_width="231dp"
        android:layout_height="20dp"
        android:layout_marginTop="44dp"
        android:text="Nome"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <TextView
        android:id="@+id/cognome_text_profilo_volontario"
        android:layout_width="231dp"
        android:layout_height="20dp"
        android:layout_marginTop="30dp"
        android:text="cognome"
        app:layout_constraintEnd_toEndOf="@+id/nome_text_profilo_volontario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nome_text_profilo_volontario"
        app:layout_constraintTop_toBottomOf="@+id/nome_text_profilo_volontario" />

    <TextView
        android:id="@+id/data_di_nascita_text_profilo_volontario"
        android:layout_width="231dp"
        android:layout_height="20dp"
        android:layout_marginTop="30dp"
        android:text="Data di nascita"
        app:layout_constraintEnd_toEndOf="@+id/cognome_text_profilo_volontario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/cognome_text_profilo_volontario"
        app:layout_constraintTop_toBottomOf="@+id/cognome_text_profilo_volontario" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="LogOut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="165dp"
        android:layout_height="181dp"
        android:layout_marginTop="148dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>